<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="yts.yourapplicationname.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@drawable/sicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/picture"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToe">
        <activity
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:name="yts.yourapplicationname.tictactoe.SettingsActivity"
            android:exported="false" />
        <activity
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:name="yts.yourapplicationname.tictactoe.MainActivity"
            android:exported="true"/>

        <activity
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:name="yts.yourapplicationname.tictactoe.MenuActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>

</manifest>